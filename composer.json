{
    "name": "quinnjr/phalcon-middleware-minimal",
    "description": "A minimal example of a working Controller::beforeExecuteRoute and Controller::afterExecuteRoute on the middle-stack Micro Phalcon framework.",
    "type": "project",
    "license": "ISC",
    "authors": [
        {
            "name": "Joseph R. Quinn",
            "email": "423821+quinnjr@users.noreply.github.com"
        }
    ],
    "require": {
        "ext-phalcon": "^4.0",
        "ext-psr": "^1.0"
    },
    "require-dev": {
        "phalcon/ide-stubs": "^4.0"
    },
    "provide": {
        "psr/log": "^1",
        "psr/cache": "^1",
        "psr/http-message": "^1",
        "psr/container": "^1",
        "psr/link": "^1",
        "psr/event-dispatcher": "^1",
        "psr/http-server-handler": "^1",
        "psr/http-server-middleware": "^1",
        "psr/simple-cache": "^1",
        "psr/http-factory": "^1",
        "psr/http-client": "^1"
    },
    "autoload": {
        "psr-4": {
            "MyApp\\": "lib/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "start": "php -S localhost:3000 -t . .htrouter.php",
        "test": "./vendor/bin/phpunit"
    }
}
